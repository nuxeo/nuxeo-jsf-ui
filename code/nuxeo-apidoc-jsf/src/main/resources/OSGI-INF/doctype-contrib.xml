<?xml version="1.0"?>
<component name="org.nuxeo.apidoc.doctypeContrib.jsf">

  <require>org.nuxeo.apidoc.doctypeContrib</require>
  <extension target="org.nuxeo.ecm.core.schema.TypeService" point="doctype">

    <doctype name="NXDistribution" append="true">
      <subtypes>
        <type>NXSeamComponent</type>
      </subtypes>
    </doctype>

    <doctype name="NXSeamComponent" extends="Document">
      <schema name="common" />
      <schema name="dublincore" />
      <schema name="file" />
      <schema name="nxseam" />

      <facet name="Commentable" />
      <facet name="Versionable" />
    </doctype>
  </extension>

</component>
