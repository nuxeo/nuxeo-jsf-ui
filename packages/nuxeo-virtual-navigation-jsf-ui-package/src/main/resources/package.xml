<?xml version="1.0"?>
<package type="addon" name="nuxeo-virtualnavigation" version="@VERSION@">
  <title>Nuxeo Virtual Navigation</title>
  <description>
    <p>Virtual Navigation plugin adds the ability to browse you repository
      content based on a specific metadata angle, provided this metadata gets
      its values from a hierarchical vocabulary.
    </p>
    <p> The plugin allows to add several new browsing axis in a tree
      representation style. Browsing axis can be configured in Studio.
    </p>
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/nuxeo-virtual-navigation/</home-page>
  <vendor>Nuxeo</vendor>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@VERSION@</platform>
    <platform>server-@VERSION@-*</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-jsf-ui</package>
  </dependencies>
  <license>Apache 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
  <visibility>PUBLIC</visibility>
</package>
